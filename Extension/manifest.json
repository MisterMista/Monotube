{
  "manifest_version": 2,
  "name": "Monotube",
  "version": "0.1",
  "description": "Stay focused on youtube",

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "run_at": "document_start",
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
          "tabs"
        ]
}
